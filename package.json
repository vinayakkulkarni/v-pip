{
  "name": "v-pip",
  "description": "Vue.js wrapper for picture-in-picture",
  "version": "2.0.0",
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "vinayak.kulkarni@live.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "private": false,
  "main": "dist/v-pip.cjs.js",
  "module": "dist/v-pip.esm.js",
  "unpkg": "dist/v-pip.js",
  "jsdelivr": "dist/v-pip.js",
  "cdn": "dist/v-pip.min.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "V-pip",
    "pip",
    "Vue.js",
    "pip",
    "Component",
    "Vue-pip"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-pip.git"
  },
  "scripts": {
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.conf.js",
    "bundle:min": "rollup -c build/rollup.min.conf.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist)/**/}*.{js,ts,vue}\"",
    "release": "shipjs prepare"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@typescript-eslint/parser": "^4.4.1",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/compiler-sfc": "^3.0.1",
    "@vue/test-utils": "^1.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "shipjs": "0.23.0",
    "typescript": "^4.0.3",
    "vue": "^3.0.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-pip/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-pip#readme"
}
